<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="259.0" prefWidth="396.0"
            stylesheets="@../../../../resources/common.css" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="library.assistant.ui.login.LoginController">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" prefHeight="67.0" prefWidth="600.0"
             style="-fx-background-color: black; -fx-text-fill: white;" text="Library Assistant Login"
             AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="18.0"/>
         </font>
      </Label>
      <JFXTextField fx:id="username" labelFloat="true" layoutX="95.0" layoutY="88.0" prefHeight="30.0" prefWidth="241.0"
                    promptText="Username">
         <font>
            <Font size="16.0"/>
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="password" labelFloat="true" layoutX="95.0" layoutY="150.0" prefHeight="33.0"
                        prefWidth="241.0" promptText="Password">
         <font>
            <Font size="17.0"/>
         </font>
      </JFXPasswordField>
      <JFXButton layoutX="95.0" layoutY="203.0" onAction="#handleLoginButtonAction" prefHeight="41.0" prefWidth="88.0"
                 text="Login"/>
      <JFXButton layoutX="216.0" layoutY="203.0" onAction="#handleCancelButton" prefHeight="41.0" prefWidth="88.0"
                 text="Cancel"/>
   </children>
</AnchorPane>
